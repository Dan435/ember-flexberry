{{#if value}}
    {{flexberry-lookup
        value=value.type
        projection="CustomizeLookupWindowExampleView"
        displayAttributeName="name"
        title=(t "forms.components-examples.flexberry-lookup.customizing-window-example.titleLookup")
        relationName="type"
        choose=choose
        remove=remove
        readonly=readonly
        limitFunction=null
        autocomplete=true
        autocompletePersistValue=true
        displayValue=value.displayValue
    }}
{{/if}}